{
	"github": {
		"silent": true
	},
	"functions": {
		"api/**/*.py": {
			"excludeFiles": "**/!(*.py)"
		}
	},
	"trailingSlash": false,
	"redirects": [
		{
			"source": "/cdn-cgi/trace",
			"destination": "https://youtube.com/watch?v=dQw4w9WgXcQ",
			"permanent": false
		}
	],
	"headers": [
		{
			"source": "/*",
			"headers": [
				{
					"key": "Access-Control-Expose-Headers",
					"value": "Date"
				}
			]
		}
	]
}
